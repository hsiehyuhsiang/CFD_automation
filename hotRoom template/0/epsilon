FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0";
    object      epsilon;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 -3 0 0 0 0];

internalField   uniform 0.0001;

boundaryField
{
    floor
    {
        type            kqRWallFunction;
        value           uniform 0.0001;
    }
    ceiling
    {
        type            fixedValue;
        value           uniform 0.0001;
    }
    fixedWalls
    {
        type            kqRWallFunction;
        value           uniform 0.0001;
    }
    buildings
    {
        type             epsilonWallFunction;
        value           uniform 0.0001;
    }
}


// ************************************************************************* //
