FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0";
    object      p_rgh;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [1 -1 -2 0 0 0 0];

internalField   uniform 0;

boundaryField
{
    floor
    {
        type            fixedFluxPressure;
        value           uniform 0;
    }

    ceiling
    {
        type            fixedFluxPressure;
        value           uniform 0;
    }

    fixedWalls
    {
        type            fixedFluxPressure;
        value           uniform 0;
    }

    buildings
    {
        type            fixedFluxPressure;
        value           uniform 0;
    }
}

